{
    "title": "Modifiers 変換",
    "rules": [
        {
            "description": "international1(\\_) -> command+option.",
            "manipulators": [
                {
                    "from": {
                        "key_code": "international1",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "left_command",
                            "modifiers": [
                                "left_option"
                            ]
                        }
                    ],
                    "type": "basic"
                }
            ]
        }
    ]
}
